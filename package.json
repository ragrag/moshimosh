{
  "name": "moshimosh",
  "version": "1.0.2",
  "author": "ragrag https://github.com/moshimosh",
  "repository": "https://github.com/ragrag/moshimosh",
  "description": "Hybrid TypeScript Node Package Creator",
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "hybrid-package",
    "esm",
    "cjs",
    "node-package",
    "package-creator",
    "boilerplate"
  ],
  "preferGlobal": true,
  "scripts": {
    "build": "tsc && node -e \"require('fs').writeFileSync('bin/index.js', '#!/usr/bin/env node\\n' + require('fs').readFileSync('bin/index.js', 'utf8'))\""
  },
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/node": "^20.14.12",
    "typescript": "^5.5.4"
  },
  "files": [
    "bin",
    "sounds",
    "template"
  ],
  "bin": {
    "moshimosh": "./bin/index.js"
  },
  "dependencies": {
    "@inquirer/prompts": "^5.3.2",
    "ansis": "^3.3.2",
    "catch-exit": "^1.2.2",
    "speaker": "^0.5.5"
  }
}
