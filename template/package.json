{
  "name": "add",
  "version": "0.0.1",
  "author": "author https://github.com/author",
  "repository": "https://github.com/author/add",
  "description": "description",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "add",
    "sum"
